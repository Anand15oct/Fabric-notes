SELECT
  CS_USERS_B1.OPRID USER_ID,
  CS_USERS_B1.OPRDEFNDESC USERNAME,
  CS_USERS_B1.EMPLID USER_EMPLOYEE_ID,
  CS_USERS_B1.OPRCLASS PL_PRIMARY_CODE,
  CS_USERS_B1.ROWSECCLASS ROW_SECURITY_CODE,
  CS_USERS_B1.PRCSPRFLCLS PROCESS_PROFILE_CODE,
  CS_USERS_B1.DEFAULTNAVHP NAVIG_HOMEPAGE_CODE,
  TO_CHAR (CS_USERS_B1.LASTPSWDCHANGE, 'YYYY-MM-DD')  LAST_PASSWORD_CHANGE,
  decode (CS_USERS_B1.ACCTLOCK, 0,'No','Yes')  ACCOUNT_LOCKED,
  CS_USERS_B1.FAILEDLOGINS FAILED_LOGINS,
  decode (CS_USERS_B1.EXPENT, 0,'No','Yes') PASSWORD_EXPIRED,
    TO_CHAR (CS_USERS_B1.LASTSIGNONDTTM, 'YYYY-MM-DD')  LAST_LOGIN_DATE,
TO_CHAR (CS_USERS_B1.LASTUPDDTTM, 'YYYY-MM-DD')  DATE_UPDATED,
  CS_USERS_B1.LASTUPDOPRID LAST_UPDATED_USER_ID,
  NVL((SELECT
  PS_UM_EMAIL_TBL_S$1.EMAIL_ADDR EMAIL_ADDR$3
FROM
  UOM_OWNER.PS_UM_EMAIL_TBL  PS_UM_EMAIL_TBL_S$1
  WHERE 
  ( PS_UM_EMAIL_TBL_S$1.EMPLID = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 ) ), NULL) USER_EMAIL,
  CS_USERS_B1.CLASSDEFNDESC PL_PRIM_DESC,
  CS_USERS_B1.CLASSDEFNDESC_1 PL_ROW_SECURITY_DESC,
  CS_USERS_B1.CLASSDEFNDESC_2 PL_PROCESS_PROFILE_DESC,
  CS_USERS_B1.CLASSDEFNDESC_3 PL_NAVIG_HOMEPAGE_DESC,
to_char((sysdate - CS_USERS_B1.LASTSIGNONDTTM),'YYYY-MM-DD')  DAYS_SINCE_LAST_LOGIN,
  NVL(PSROLEUSER.ROLENAME, NULL) ROLE_ID,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.STAFF_FLAG STAFF_FLAG$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID ) AND
  ( rownum = 1 ) ) STAFF_FLAG,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LEAVER_FLAG LEAVER_FLAG$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID ) AND
  ( rownum = 1 ) ) LEAVER_FLAG,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.CURRENT_STAFF_FLAG CURRENT_STAFF_FLAG$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID ) AND
  ( rownum = 1 ) ) CURRENT_STAFF_FLAG,
(SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.STAFF_LEAVING_DATE STAFF_LEAVING_DATE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID ) AND
  ( rownum = 1 ) ) STAFF_LEAVING_DATE,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.FACULTY_CODE FACULTY_CODE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID ) AND
  ( rownum = 1 ) ) FACULTY_CODE,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.FACULTY_DESCR FACULTY_DESCR$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 ) )  FACULTY_DESCR,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LEVEL_3_CODE LEVEL_3_CODE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 )  ) LEVEL_3_CODE,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LEVEL_3_DESCR LEVEL_3_DESCR$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 )  ) LEVEL_3_DESCR,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LEVEL_4_CODE LEVEL_4_CODE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 )  ) LEVEL_4_CODE,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LEVEL_4_DESCR LEVEL_4_DESCR$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 )  ) LEVEL_4_DESCR,
(SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.LAST_POST_HOLDING_CHANGE_DT LAST_POST_HOLDING_CHANGE_DT$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 ))  LAST_POST_HOLDING_CHANGE_DT,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.STAFF_ACCESS_AFTER_LEAVE STAFF_ACCESS_AFTER_LEAVE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )  AND
  ( rownum = 1 )  ) STAFF_ACCESS_AFTER_LEAVE,
  NVL((SELECT
  'Y' STUDENT_FLAG$3
FROM
  UOM_OWNER.STUDENT_PROGRAM_FC_SR  STUDENT_PROGRAM_FC_SR_S$1
  WHERE 
  ( STUDENT_PROGRAM_FC_SR_S$1.PROGRAM_STATUS =  'AC' )   AND
  ( STUDENT_PROGRAM_FC_SR_S$1.EMPLID = CS_USERS_B1.EMPLID )   AND
  ( STUDENT_PROGRAM_FC_SR_S$1.CURRENT_FLAG = 'Y' )  AND
  ( rownum = 1 ) ), 'N') STUDENT_FLAG,
  CASE WHEN  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.STAFF_FLAG STAFF_FLAG$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )   AND
  ( rownum = 1 ) ) ='Y' AND  (NVL((SELECT
  'Y' STUDENT_FLAG$3
FROM
  UOM_OWNER.STUDENT_PROGRAM_FC_SR  STUDENT_PROGRAM_FC_SR_S$1
  WHERE 
  ( STUDENT_PROGRAM_FC_SR_S$1.PROGRAM_STATUS = 'AC' )   AND
  ( STUDENT_PROGRAM_FC_SR_S$1.EMPLID = CS_USERS_B1.EMPLID )   AND
  ( STUDENT_PROGRAM_FC_SR_S$1.CURRENT_FLAG = 'Y' )  AND
  ( rownum = 1 ) ), 'N')) ='Y' 
THEN 'Y' 
ELSE 'N' 
END  STAFF_STUDENT_FLAG,
(SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.ORIG_START_DATE ORIG_START_DATE$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )  AND
  ( rownum = 1 ) ) ORIG_START_DATE,
  NVL((SELECT
  CS_USERS_CAREER_SEC_S$1.UGRD_ACCESS UGRD_ACCESS$3
FROM
  UOM_OWNER.CS_USERS_CAREER_SEC  CS_USERS_CAREER_SEC_S$1
  WHERE 
  ( CS_USERS_CAREER_SEC_S$1.OPRID = CS_USERS_B1.OPRID )   AND
  ( rownum = 1 )  ), NULL) UGRD_ACCESS,
  NVL((SELECT
  CS_USERS_CAREER_SEC_S$1.MDUG_ACCESS MDUG_ACCESS$3
FROM
  UOM_OWNER.CS_USERS_CAREER_SEC  CS_USERS_CAREER_SEC_S$1
  WHERE 
  ( CS_USERS_CAREER_SEC_S$1.OPRID = CS_USERS_B1.OPRID )   AND
  ( rownum = 1 )  ), NULL) MDUG_ACCESS,
  NVL((SELECT
  CS_USERS_CAREER_SEC_S$1.PGDT_ACCESS PGDT_ACCESS$3
FROM
  UOM_OWNER.CS_USERS_CAREER_SEC  CS_USERS_CAREER_SEC_S$1
  WHERE 
  ( CS_USERS_CAREER_SEC_S$1.OPRID = CS_USERS_B1.OPRID )  AND
  ( rownum = 1 )  ), NULL) PGDT_ACCESS,
  NVL((SELECT
  CS_USERS_CAREER_SEC_S$1.PGDR_ACCESS PGDR_ACCESS$3
FROM
  UOM_OWNER.CS_USERS_CAREER_SEC  CS_USERS_CAREER_SEC_S$1
  WHERE 
  ( CS_USERS_CAREER_SEC_S$1.OPRID = CS_USERS_B1.OPRID )   AND
  ( rownum = 1 )  ), NULL) PGDR_ACCESS,
  NVL((SELECT
  CS_USERS_CAREER_SEC_S$1.CEPD_ACCESS CEPD_ACCESS$3
FROM
  UOM_OWNER.CS_USERS_CAREER_SEC  CS_USERS_CAREER_SEC_S$1
  WHERE 
  ( CS_USERS_CAREER_SEC_S$1.OPRID = CS_USERS_B1.OPRID )   AND
  ( rownum = 1 )  ), NULL) CEPD_ACCESS,
  NVL((SELECT
  PS_SCRTY_TBL_INST_S$1.ACCESS_CD ACCESS_CD$3
FROM
  UOM_OWNER.PS_SCRTY_TBL_INST  PS_SCRTY_TBL_INST_S$1
  WHERE 
  ( PS_SCRTY_TBL_INST_S$1.OPRID = CS_USERS_B1.OPRID )   AND
  ( rownum = 1 )  ), NULL) ACCESS_CD,
  (SELECT
  ICC_ALL_STAFF_CS_SEC_S$1.HR_NAME HR_NAME$3
FROM
  UOM_OWNER.ICC_ALL_STAFF_CS_SEC  ICC_ALL_STAFF_CS_SEC_S$1
  WHERE 
  ( ICC_ALL_STAFF_CS_SEC_S$1.EMPLOYEE_NUMBER = CS_USERS_B1.EMPLID )  AND
  ( rownum = 1 )  ) HR_NAME$2
FROM
    UOM_OWNER.PSROLEUSER  PSROLEUSER   
RIGHT OUTER JOIN   UOM_OWNER.CS_USERS_B1  CS_USERS_B1 ON ( ( PSROLEUSER.ROLEUSER = CS_USERS_B1.OPRID ) )
